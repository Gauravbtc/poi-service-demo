  "BrandImport": {
    "post": {
      "summary": "Brand Import 「It is One time Process」",
      "description": "We are going to import 「Brand＿Master」 from prismatix brand api",
      "produces": [
        "application/json"
      ],
      "consumes": [
        "multipart/form-data"
      ],
      "tags": [
        "Brand"
      ],
      "requestBody": {
         "$ref": "brand_parameters.json#/requestBodies/BrandImportRequest"
      },
      "responses": {
        "200": {
          "$ref": "brand_responses.json#/responses/BrandImportedSuccessfully"
         },
        "422": {
          "$ref": "brand_responses.json#/responses/BrandParameterFailure"
        }
      }
    }
  },
  "BrandList": {
    "get": {
      "summary": "All Brand Lisitng",
